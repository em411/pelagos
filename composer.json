{
    "name": "griidc/pelagos",
    "license": "BSD",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [
            "app/AppKernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "": "tests/helpers"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^1.4",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/monolog-bundle": "^3.4",
        "sensio/framework-extra-bundle": "^5.4",
        "friendsofsymfony/rest-bundle": "^2.3",
        "jms/serializer-bundle": "^2.0",
        "nelmio/api-doc-bundle": "^2.11",
        "willdurand/hateoas-bundle": "^1.1",
        "simplethings/entity-audit-bundle": "^1.0",
        "friendsofsymfony/jsrouting-bundle": "^2.1",
        "php-amqplib/rabbitmq-bundle": "^1.9",
        "jsor/doctrine-postgis": "^1.1",
        "opensoft/doctrine-postgres-types": "^1.2",
        "phobetor/rabbitmq-supervisor-bundle": "^3.0",
        "friendsofsymfony/elastica-bundle": "^5.1.1",
        "phayes/geophp": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "symfony/webpack-encore-bundle": "^1.1",
        "php-http/httplug-bundle": "^1.10",
        "php-http/guzzle6-adapter": "^1.1",
        "guzzlehttp/guzzle": "~6.0",
        "kriswallsmith/buzz": "0.16.*",
        "hylianshield/base32-crockford": "^1.0",
        "symfony/asset": "4.3.*",
        "symfony/orm-pack": "^1.0",
        "symfony/twig-bundle": "4.3.*",
        "symfony/form": "4.3.*",
        "symfony/security-bundle": "4.3.*",
        "symfony/translation": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/profiler-pack": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "symfony/web-server-bundle": "^4.0",
        "squizlabs/php_codesniffer": "2.7.*",
        "phpunit/phpunit": "7.4.*",
        "symfony/dotenv": "4.3.*",
        "symfony/maker-bundle": "^1.13",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "symfony/debug-bundle": "4.3.*"
    },
     "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
