[
    {% for fundingCycle in YR1 %}
    {
        "data": {
            "title": "{{fundingCycle.getName}}",
            "attr": {
                "style": "color:{{tree.yr1_color}}"
            }
        },
        "attr": {
            "id": "projects_funding-cycle_{{fundingCycle.ID}}",
            "action": "{{tree.yr1_action | evaluate}}"
        }
        {% if tree.max_depth > 0 %}
        ,
            {% if tree.expand_to_depth > 0 %}
                "state": "open"
            {% else %}
                "state": "closed"
            {% endif %}
        {% endif %}
    }
    {% if not loop.last %}
    ,
    {% endif %}
    {% endfor %}
]
