[
{% for person in people %}
    {
        "data": {
            "title": "{{person.LastName | raw}}, {{person.FirstName | raw}} <span class='treeInstName'>({{person.Institution_Name | raw}})</span>",
            "attr": {
                "style": "color:#00A",
                "href": "javascript:showProjects(\"peopleId\",{{person.ID}});"
            }
        },
        "attr": {
            "id": "projects/peopleId/{{person.ID}}"
        }
    }
    {% if not loop.last %}
    ,
    {% endif %}
{% endfor %}
]
