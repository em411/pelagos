[
{% for person in people %}
    {
        "data": {
            "title": "{{person.LastName | raw}}, {{person.FirstName | raw}} <span class='treeInstName'>({{person.Institution_Name | raw}})</span>",
            "attr": {
                "style": "color:{{tree.researcher_color}}"
                ,
                "href": "{{tree.researcher_link | evaluate}}"
            }
        },
        "attr": {
            "id": "projects_peopleId_{{person.ID}}"
        }
        {% if tree.max_depth > 0 %}
        ,
            {% if tree.expand_to_depth > 0 %}
                "state": "open"
            {% else %}
                "state": "closed"
            {% endif %}
        {% endif %}
    }
    {% if not loop.last %}
    ,
    {% endif %}
{% endfor %}
]
