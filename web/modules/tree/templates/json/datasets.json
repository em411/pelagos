[
{% for dataset in datasets %}
    {
        "data": {
            "title": "{{dataset.title | raw}}",
            "attr": {
                "style": "color:{{tree.dataset_color}}",
                "href": "{{tree.dataset_link | evaluate}}"
            }
        }
        ,
        "attr": {
            "id": "datasets_ID_{{dataset.udi}}"
        }
    }
    {% if not loop.last %}
    ,
    {% endif %}
{% endfor %}
]
