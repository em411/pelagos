[
    {% for fundSrc in otherSources %}
    {
        "data": {
            "title": "{{fundSrc.Name}}{% if fundSrc.dataset_count %} <span style='color:black;font-weight:bold;'>({{fundSrc.dataset_count}})</span>{% endif %}",
            "attr": {
                "style": "color:{{tree.other_sources_color}}"
            }
        },
        "attr": {
            "id": "projects_fundSrc_{{fundSrc.ID}}",
            "action": "{{tree.other_sources_action | evaluate}}"
        }
        {% if tree.max_depth > 0 %}
        ,
            {% if tree.expand_to_depth > 0 %}
                "state": "open"
            {% else %}
                "state": "closed"
            {% endif %}
        {% endif %}
    }
    {% if not loop.last %}
    ,
    {% endif %}
    {% endfor %}
]
