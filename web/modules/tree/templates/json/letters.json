[
{% for letter in letters %}
    {
        "data": {
            "title": "{{letter.Letter}}{% if letter.dataset_count %} <span style='color:black;font-weight:bold;'>({{letter.dataset_count}})</span>{% endif %}",
            "attr": {
                "style": "color:black"
            }
        },
        "attr": {
            "id": "{{letter.Letter}}"
        },
        {% if expand_to_depth > 0 %}
            "state": "open"
        {% else %}
            "state": "closed"
        {% endif %}
    }
    {% if not loop.last %}
    ,
    {% endif %}
{% endfor %}
]
